deviation_8(arg1: int[], arg2: int[], arg3: int[], arg4: int[], arg5: int[], arg6: int[], arg7: int[], arg8: int[]): int, int, int, int, int, int, int, int{
    sum: int = 0
    mean: int = 0

    sum = sum + arg1[0]
    sum = sum + arg2[5]
    sum = sum + arg3[6]
    sum = sum + arg4[2]
    sum = sum + arg5[3]
    sum = sum + arg6[2]
    sum = sum + arg7[0]
    sum = sum + arg8[4]

    mean = sum / 8

    return arg1[0] - mean, arg2[5] - mean, arg3[6] - mean, arg4[2] - mean, arg5[3] - mean, arg6[2] - mean, arg7[0] - mean, arg8[4] - mean
}


